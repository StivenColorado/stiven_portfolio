import KeyboardIcon from "./icons/KeyboardIcon.astro";
const items = [
    {
        date: "Agosto 1 - Actualmente",
        title: "Practicante Desarrollador de Software - Sena sennova",
        description:
            "Actualmente hago parte del equipo sennova brindando apoyo en el area digital | Python | Javascript",
        link: "#",
    },
    {
        date: "Abril 2024 - Junio 2024",
        title: "Practicante Desarrollador de Software - Accedo Technologies",
        description:
            "Participe como practicante en Accedo Technologies con el rol de desarrollador de software. uso de Laravel - php | VueJs",
        link: "#",
    },
    {
        date: "Diciembre 2023",
        title: "Desarrollador Backend",
        contact: "+57 317 5404432",
        description:
            "Colorabore en un proyecto para el registro de asistencia de eventos. agregacion de contactos, lectura de QR.",
        link: "#",
    },
    {
        date: "Abril 2021",
        title: "Desarrollador en Isilab Foundation",
        description:
            "Hice parte en el Desarrollo de juegos logico-matematicos para ni√±os con discapacidades cognitivas.",
        link: "#",
    },
];
---

<ol class="relative border-s border-gray-200 dark:border-gray-700 pl-3">
    {
        items.map((item, index) => (
            <li class="mb-10 ms-6">
                <ExperienceItem {...item}>
                    {console.log(index)}
                    {index === 0 && <BrainIcon />}
                    {index === 1 && <KeyboardIcon />}
                    {index === 2 && <QrIcon />}
                    {index === 3 && <Cube />}
                </ExperienceItem>
            </li>
        ))
    }
</ol>
